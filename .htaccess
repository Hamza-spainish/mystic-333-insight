RewriteEngine On

# Force HTTPS + www
RewriteCond %{HTTPS} !=on [OR]
RewriteCond %{HTTP_HOST} ^theangelnumber333\.com$ [NC]
RewriteRule ^ https://www.theangelnumber333.com%{REQUEST_URI} [L,R=301,NE]

# Keep real files/folders untouched
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# SPA fallback
RewriteRule ^ index.html [L]
